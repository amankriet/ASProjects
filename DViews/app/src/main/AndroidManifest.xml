        </activity>
        <activity android:name=".TextView"></activity>
        <activity android:name=".RatingBarView"></activity>
        <activity android:name=".ToggleButtonView"></activity>
        <activity android:name=".ImageButtonview"></activity>
        <activity android:name=".CheckBoxXView"></activity>
        <activity android:name=".RadioButton"></activity>
        <activity android:name=".ButtonView"></activity>
        <activity android:name=".EditText"></activity>
    </application>

</manifest>